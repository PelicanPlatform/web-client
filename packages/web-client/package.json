{
    "name": "@pelicanplatform/web-client",
    "version": "1.0.21",
    "homepage": "https://pelicanplatform.org",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/PelicanPlatform/web-client.git"
    },
    "description": "Pelican web client",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "jest --coverage --json --outputFile=.github/tests/test-summary.json",
        "build": "tsc --project ./tsconfig.json && node scripts/prepare-dist-package.js",
        "prepublish": "npm run build"
    },
    "exports": {
        ".": {
            "import": {
              "development": "./src/index.ts",
              "default": "./dist/index.js"
            },
            "require": "./dist/index.cjs"
        }
    },
    "publishConfig": {
      "exports": {
        ".": {
          "import": "./dist/index.js",
          "require": "./dist/index.cjs"
        }
      }
    },
    "files": [
        "dist/*",
        "README.md",
        "package.json"
    ],
    "author": "",
    "license": "Apache-2.0",
    "devDependencies": {
        "@babel/preset-typescript": "^7.26.0",
        "@jest/globals": "^29.7.0",
        "@types/jest": "^29.5.8",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^24.9.1",
        "i": "^0.3.7",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "ts-jest": "^29.1.1",
        "typescript": "^5.2.2"
    }
}
